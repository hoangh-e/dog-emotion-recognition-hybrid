        "            \n",
        "            print(f\"   üèÜ Predicted: {predicted_emotion} {'‚úÖ' if correct else '‚ùå'}\")\n",
        "        else:\n",
        "            print(\"   ‚ùå Prediction failed\")\n",
        "    except Exception as e:\n",
        "        print(f\"   ‚ùå Error: {e}\")\n",
        "\n",
